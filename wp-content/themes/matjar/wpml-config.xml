        </widget>
		<widget name="matjar_wc_vendors">
            <fields>
                <field type="WC Vendors - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_hot_deal_products">
            <fields>
                <field type="Hot Deal Products - Title" editor_type="LINE">title</field>
                <field type="Hot Deal Products - Deal Title" editor_type="LINE">deal_title</field>
            </fields>
        </widget>
		<widget name="matjar_product_brands">
            <fields>
                <field type="Product Brands - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_product_categories">
            <fields>
                <field type="Product Categories - Title" editor_type="LINE">title</field>
                <field type="Product Categories - Button Text" editor_type="LINE">view_more_button_text</field>
				<field type="Product Categories - Button Link" editor_type="LINK" key_of="view_more_button_link">url</field>
            </fields>
        </widget>
		<widget name="matjar_product_categories_thumbnail">
            <fields>
                <field type="Product Categories Thumbnail - Title" editor_type="LINE">title</field>
