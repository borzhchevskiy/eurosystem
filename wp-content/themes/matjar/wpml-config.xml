            </fields>
        </widget>
		<widget name="matjar-custom-categories">
			<fields-in-item items_of="category_list">
				<field type="Product Custom Categories - Title" editor_type="LINE">cat_title</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_products_and_categories_box">
            <fields>
                <field type="Products And Categories Box - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_products_category_tabs">
            <fields>
                <field type="Products Category Tabs - Title" editor_type="LINE">title</field>
            </fields>
			<fields-in-item items_of="product_tabs">
				<field type="Products Category Tabs - Tab Title" editor_type="LINE">tab_title</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_products_tabs">
            <fields>
                <field type="Products Tabs - Title" editor_type="LINE">title</field>
            </fields>
			<fields-in-item items_of="product_tabs">
				<field type="Products Tabs - Tab Title" editor_type="LINE">tab_title</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_products_grid_carousel">
            <fields>
                <field type="Products Grid Or Carousel - Title" editor_type="LINE">title</field>
                <field type="Products Grid Or Carousel - Button Text" editor_type="LINE">view_more_button_text</field>
				<field type="Products Grid Or Carousel - Button Link" editor_type="LINK" key_of="view_more_button_link">url</field>
            </fields>
        </widget>
		<widget name="matjar_products_recently_viewed">
            <fields>
                <field type="Products Recently Viewed - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_products_widget">
            <fields>
                <field type="Products Widget - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_products_with_banner">
            <fields>
                <field type="Products With Banner - Title" editor_type="LINE">title</field>
                <field type="Products With Banner - Button Text" editor_type="LINE">view_more_button_text</field>
				<field type="Products With Banner - Button Link" editor_type="LINK" key_of="view_more_button_link">url</field>
            </fields>
