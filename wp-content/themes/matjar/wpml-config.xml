	<custom-fields>
		<custom-field action="translate">_themejr_post_format_video</custom-field>
		<custom-field action="translate">_themejr_post_format_audio</custom-field>
		<custom-field action="translate">_themejr_post_format_quote</custom-field>
		<custom-field action="translate">_themejr_post_format_quote_author</custom-field>
		<custom-field action="translate">_themejr_post_format_quote_author_url</custom-field>
		<custom-field action="translate">_themejr_post_format_link_text</custom-field>
		<custom-field action="translate">_themejr_client_name</custom-field>		
		<custom-field action="translate">_themejr_website_link</custom-field>
        <custom-field action="translate">_themejr_product_video</custom-field>
        <custom-field action="translate">_themejr_product_custom_tab_heading</custom-field>
        <custom-field action="translate">_themejr_product_custom_tab_content</custom-field>
        <custom-field action="translate">_themejr_custom_page_title</custom-field>
    </custom-fields>
	<elementor-widgets>		
		<widget name="matjar-banner">
            <fields>
                <field type="Banner - Title" editor_type="LINE">title</field>
                <field type="Banner - Subtitle" editor_type="AREA">subtitle</field>
                <field type="Banner - Description" editor_type="AREA">banner_content</field>
                <field type="Banner - Button Text" editor_type="LINE">button_text</field>
				<field type="Banner - Link" editor_type="LINK" key_of="banner_link">url</field>
            </fields>
        </widget>
		<widget name="matjar-banner">
            <fields>
                <field type="Banner - Title" editor_type="LINE">title</field>
                <field type="Banner - Subtitle" editor_type="AREA">subtitle</field>
                <field type="Banner - Description" editor_type="AREA">banner_content</field>
                <field type="Banner - Button Text" editor_type="LINE">button_text</field>
				<field type="Banner - Link" editor_type="LINK" key_of="banner_link">url</field>
            </fields>
        </widget>
		<widget name="matjar_banner_carousel">
			<fields-in-item items_of="banners">
				<field type="Banner Carousel - Title" editor_type="LINE">title</field>
                <field type="Banner Carousel - Subtitle" editor_type="AREA">subtitle</field>
                <field type="Banner Carousel - Description" editor_type="AREA">banner_content</field>
                <field type="Banner Carousel - Button Text" editor_type="LINE">button_text</field>
				<field type="Banner Carousel - Link" editor_type="LINK" key_of="banner_link">url</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_blog_carousel">
            <fields>
                <field type="Blog Carousel - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar-button">
            <fields>
                <field type="Button - Text" editor_type="LINE">text</field>
                <field type="Button - Link" editor_type="LINK" key_of="button_link">url</field>
            </fields>
        </widget>
		<widget name="matjar_contactus">
            <fields>
                <field type="Contactus - Title" editor_type="LINE">title</field>
                <field type="Contactus - Description" editor_type="AREA">description</field>
            </fields>
        </widget>
		<widget name="matjar_counter">
            <fields>
                <field type="Counter - Title" editor_type="LINE">counter_title</field>
                <field type="Counter - Suffix" editor_type="LINE">counter_suffix</field>
                <field type="Counter - Prefix" editor_type="LINE">counter_prefix</field>
            </fields>
        </widget>
		<widget name="matjar-heading">
            <fields>
				<field type="Heading - Title" editor_type="LINE">title</field>
                <field type="Heading - Subtitle" editor_type="LINE">sub_title</field>
                <field type="Heading - Tagline" editor_type="AREA">tagline</field>
            </fields>
        </widget>
		<widget name="matjar-infobox">
            <fields>
                <field type="Infobox - Subtitle" editor_type="LINE">subtitle</field>
                <field type="Infobox - Title" editor_type="LINE">title_text</field>
                <field type="Infobox - Description" editor_type="AREA">description</field>
                <field type="Infobox - Link" editor_type="LINK" key_of="link">url</field>
                <field type="Infobox - Read More Text" editor_type="LINE">read_more_text</field>
            </fields>
        </widget>
		<widget name="matjar_instagram">
            <fields>
                <field type="Instagram - Title" editor_type="LINE">title</field>
                <field type="Instagram - Link" editor_type="LINK" key_of="profile_link">url</field>
            </fields>
        </widget>
		<widget name="matjar-menu-block">
            <fields>
                <field type="Mega Menu Item - Title" editor_type="LINE">title</field>
                <field type="Mega Menu Item - Link" editor_type="LINK" key_of="link">url</field>
				<field type="Mega Menu Item - Label" editor_type="LINE">label_text</field>
            </fields>
			<fields-in-item items_of="menu_items">
				<field type="Sub Menu Item - Title" editor_type="LINE">title</field>
                <field type="Sub Menu Item - Link" editor_type="LINK" key_of="link">url</field>
				<field type="Sub Menu Item - Label" editor_type="LINE">label_text</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_newsletter">
            <fields>
                <field type="Newsletter - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_portfolio_carousel">
            <fields>
                <field type="Portfolio Carousel - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_progress_bar">
            <fields>
                <field type="Progress Bar - Title" editor_type="LINE">title</field>
                <field type="Progress Bar - Units" editor_type="LINE">units</field>
            </fields>
			<fields-in-item items_of="bar_items">
				<field type="Progress Bar - Label" editor_type="LINE">bar_label</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_social_buttons">
            <fields>
                <field type="Social Buttons - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_team">            
			<fields-in-item items_of="team_members">
				<field type="Team - Name" editor_type="LINE">name</field>
				<field type="Team - Designation" editor_type="LINE">designation</field>
				<field type="Team - Description" editor_type="AREA">description</field>
				<field type="Team - Facebook" editor_type="LINE">facebook</field>
				<field type="Team - Twitter" editor_type="LINE">twitter</field>
				<field type="Team - Linkedin" editor_type="LINE">linkedin</field>
				<field type="Team - Skype" editor_type="LINE">skype</field>
				<field type="Team - Instagram" editor_type="LINE">instagram</field>
				<field type="Team - Youtube" editor_type="LINE">youtube</field>
				<field type="Team - Facebook" editor_type="LINE">facebook</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_testimonials">            
			<fields-in-item items_of="testimonials_items">
				<field type="Testimonials - Name" editor_type="LINE">name</field>
				<field type="Testimonials - Description" editor_type="AREA">description</field>
				<field type="Testimonials - Designation" editor_type="LINE">designation</field>
			</fields-in-item>
        </widget>
		<widget name="matjar_vertical_menu">
            <fields>
                <field type="Categories Menu - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
		<widget name="matjar_dokan_vendors">
            <fields>
                <field type="Dokan Vendors - Title" editor_type="LINE">title</field>
            </fields>
        </widget>
